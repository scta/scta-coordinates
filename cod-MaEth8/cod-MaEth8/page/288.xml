            </TextLine>
            <TextLine id="r1l33" custom="readingOrder {index:28;}">
                <Coords points="203,1671 1861,1637 1860,1589 202,1627"/>
                <Baseline points="212,1658 293,1656 375,1655 457,1653 539,1652 621,1650 703,1647 785,1646 867,1643 949,1641 1031,1638 1112,1637 1194,1634 1276,1632 1358,1630 1440,1628 1522,1626 1604,1623 1686,1622 1768,1620 1850,1619"/>
                <TextEquiv>
                    <Unicode>transgressionum reipublicae est popularis potestas: quandoquidem parum reipublicae transgreditur speciem</Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="r1l34" custom="readingOrder {index:29;}">
                <Coords points="202,1714 1854,1677 1817,1627 201,1669"/>
