                <Coords points="232,3320 1906,3304 1905,3237 1859,3235 232,3251"/>
                <Baseline points="242,3288 324,3288 407,3288 489,3288 572,3288 655,3287 737,3287 820,3287 903,3285 985,3285 1068,3284 1151,3284 1233,3282 1316,3282 1399,3281 1481,3279 1564,3279 1647,3278 1729,3276 1812,3276 1895,3275"/>
                <TextEquiv>
