                <Coords points="513,3257 2075,3260 2264,3245 2265,3181 513,3176"/>
                <Baseline points="524,3216 610,3217 697,3219 783,3219 870,3220 956,3221 1043,3221 1129,3222 1216,3222 1302,3222 1389,3222 1475,3222 1562,3222 1648,3222 1735,3222 1821,3222 1908,3221 1994,3221 2081,3221 2167,3221 2254,3221"/>
