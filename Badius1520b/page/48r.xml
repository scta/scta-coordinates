                <TextEquiv>
                    <Unicode>Deus autem nullius boni habet defectum, quia nullum expectat. Similiter timor, quia est respectu mali</Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="r1l28" custom="readingOrder {index:25;}">
                <Coords points="129,1485 1607,1495 1583,1444 129,1446"/>
