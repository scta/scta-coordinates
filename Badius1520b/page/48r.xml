                <TextEquiv>
                    <Unicode>nes praedictae solummodo attribuuntur deo, vt dicatur iratus quia ex ordine iustitiae voluntarie punit.</Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="r1l36" custom="readingOrder {index:33;}">
                <Coords points="111,1851 1608,1864 1608,1816 1532,1807 111,1799"/>
